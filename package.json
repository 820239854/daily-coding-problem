{
  "name": "daily-coding-problem",
  "version": "1.0.0",
  "scripts": {},
  "devDependencies": {
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
